CC = gcc
CFLAGS = -I.
DEPS = 202101074_Lab4_1_myheader.h
OBJ = 202101074_Lab4_1_fact.o 202101074_Lab4_1_pow.o

%.o: %.c $(DEPS)
	$(CC) -c -o $@ $< $(CFLAGS)
	
libmy_dynamic.so: $(OBJ)
	$(CC) -shared -o $@ $^
	
202101074_Lab4_3a.out: 202101074_Lab4_1_main.o libmy_dynamic.so
	$(CC) -o $@ $^ $(CFLAGS)
